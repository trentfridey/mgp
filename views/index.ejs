<html ng-app="mgpHandler">
  <head>
    <title>Welcome - MGP</title>
    <link rel="stylesheet" type="text/css" href="style.css"/>
  </head>
  <body>
    <div class="container">
    <%- include('partials/navbar') %>
      <center>
        <div class="content" ng-controller="mgpGameCtrl">
          <h1>
            Game Board
          </h1>
          <div class="board">
            <canvas id="grid" height="360px" width="840px"></canvas>
            <canvas id="canvas" height="360px" width="840px"></canvas>
          </div>
          <div class="dashboard">
            <div class="btn" id="start">
              Start
            </div>
            <div class="btn" id="stop">
              Stop
            </div>
            <div class="btn" id="reset">
              Reset
            </div>
          </div>
        </div>
      </center>
    <div class="push"></div> <!-- For sticky footer --->
    </div>
    <div class="footer">
    Made by Trent Fridey 2017
  </div>
    <footer>
      <script
			  src="https://code.jquery.com/jquery-3.2.1.min.js"
			  integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
			  crossorigin="anonymous"></script>
      <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js"></script>
      <script src="app.js"></script>
      <script src="client.js"></script> 
    </footer>
  </body>
  
</html>
